generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  Id        Int      @id @default(autoincrement())
  FirstName String
  LastName  String
  Email     String   @unique
  Password  String?
  Cpf       String
  Age       Int
  CreatedAt DateTime @default(now())
  UpdatedAt DateTime @updatedAt
}
